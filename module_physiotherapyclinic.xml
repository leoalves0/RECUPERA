<?xml version="1.0" encoding="UTF-8"?>
<project name="module_physiotherapyclinic" default="compile.module.physiotherapyclinic">
  <dirname property="module.physiotherapyclinic.basedir" file="${ant.file.module_physiotherapyclinic}"/>
  
  <property name="module.jdk.home.physiotherapyclinic" value="${jdk.home.1.8}"/>
  <property name="module.jdk.bin.physiotherapyclinic" value="${jdk.bin.1.8}"/>
  <property name="module.jdk.classpath.physiotherapyclinic" value="jdk.classpath.1.8"/>
  
  <property name="compiler.args.physiotherapyclinic" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="physiotherapyclinic.output.dir" value="${module.physiotherapyclinic.basedir}/bin"/>
  <property name="physiotherapyclinic.testoutput.dir" value="${module.physiotherapyclinic.basedir}/bin"/>
  
  <path id="physiotherapyclinic.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="physiotherapyclinic.module.production.classpath">
    <path refid="${module.jdk.classpath.physiotherapyclinic}"/>
    <pathelement location="${idea.home}/lib/junit-4.12.jar"/>
  </path>
  
  <path id="physiotherapyclinic.runtime.production.module.classpath">
    <pathelement location="${physiotherapyclinic.output.dir}"/>
    <pathelement location="${idea.home}/lib/junit-4.12.jar"/>
  </path>
  
  <path id="physiotherapyclinic.module.classpath">
    <pathelement location="${physiotherapyclinic.output.dir}"/>
    <path refid="${module.jdk.classpath.physiotherapyclinic}"/>
    <pathelement location="${idea.home}/lib/junit-4.12.jar"/>
  </path>
  
  <path id="physiotherapyclinic.runtime.module.classpath">
    <pathelement location="${physiotherapyclinic.output.dir}"/>
    <pathelement location="${idea.home}/lib/junit-4.12.jar"/>
  </path>
  
  
  <patternset id="excluded.from.module.physiotherapyclinic">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.physiotherapyclinic">
    <patternset refid="excluded.from.module.physiotherapyclinic"/>
  </patternset>
  
  <path id="physiotherapyclinic.module.sourcepath">
    <dirset dir="${module.physiotherapyclinic.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.physiotherapyclinic" depends="compile.module.physiotherapyclinic.production,compile.module.physiotherapyclinic.tests" description="Compile module PhysiotherapyClinic"/>
  
  <target name="compile.module.physiotherapyclinic.production" depends="register.custom.compilers" description="Compile module PhysiotherapyClinic; production classes">
    <mkdir dir="${physiotherapyclinic.output.dir}"/>
    <javac2 destdir="${physiotherapyclinic.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.physiotherapyclinic}/javac">
      <compilerarg line="${compiler.args.physiotherapyclinic}"/>
      <bootclasspath refid="physiotherapyclinic.module.bootclasspath"/>
      <classpath refid="physiotherapyclinic.module.production.classpath"/>
      <src refid="physiotherapyclinic.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.physiotherapyclinic"/>
    </javac2>
    
    <copy todir="${physiotherapyclinic.output.dir}">
      <fileset dir="${module.physiotherapyclinic.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.physiotherapyclinic.tests" depends="register.custom.compilers,compile.module.physiotherapyclinic.production" description="compile module PhysiotherapyClinic; test classes" unless="skip.tests"/>
  
  <target name="clean.module.physiotherapyclinic" description="cleanup module">
    <delete dir="${physiotherapyclinic.output.dir}"/>
    <delete dir="${physiotherapyclinic.testoutput.dir}"/>
  </target>
</project>